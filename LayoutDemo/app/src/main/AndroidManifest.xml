<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mypc.layoutdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.car_detail" />
        <activity android:name=".activity.BrandList" />
        <activity android:name=".activity.ListByBrands" />
        <activity android:name=".activity.ForgotPassword" />
        <activity android:name=".activity.Login" />
        <activity android:name=".activity.SearchForm" />
        <activity android:name=".activity.Profile" />
        <activity android:name=".activity.ArticleUp" />
        <activity android:name=".activity.Contact" />
        <activity android:name=".activity.ArticleRegister" />
        <activity android:name=".activity.AdvancedSearch" />
        <activity android:name=".activity.ArticleDetail"/>
    </application>

</manifest>